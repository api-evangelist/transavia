{
    "openapi": "3.0.0",
    "info": {
        "version": "",
        "title": "Routes API v3",
        "description": "Returns all available routes\r\n\r\n",
        "termsOfService": ""
    },
    "servers": [
        "httpstst.api.transavia.com\/v3\/routes"
    ],
    "paths": {
        "\/": {
            "get": {
                "summary": "Get all routes by origin and\/or destination",
                "description": "Retrieves all routes.",
                "operationId": "58d3ca3eab95b50fe4da10fd",
                "deprecated": 0,
                "parameters": [
                    {
                        "name": "Origin",
                        "in": "query",
                        "description": "IATA-code (3 characters) of the origin airport. Example: AMS"
                    },
                    {
                        "name": "Destination",
                        "in": "query",
                        "description": "IATA-code (3 characters) of the destination airport. Example: BCN"
                    }
                ]
            }
        },
        "\/{id}": {
            "get": {
                "summary": "Get route by id",
                "description": "Gets the route. Note, this request doesn't have a data property in the response.",
                "operationId": "58d3ca3eab95b50fe4da10fe",
                "deprecated": 0,
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Route identifier, {origin airport IATA-code}-{destination airport IATA-code}. Example: AMS-BCN."
                    }
                ]
            }
        }
    },
